<?xml version="1.0" encoding="utf-8" ?>
<odoo noupdate="1">
    <record id="seq_pettty_cash" model="ir.sequence">
        <field name="name">Sequence Petty Cash</field>
        <field name="code">asft.pettycash</field>
        <field name="prefix">PTC/%(range_year)s/%(month)s/</field>
        <field name="padding">3</field>
        <field name="use_date_range" eval="True"/>
    </record>
    <record id="seq_pettty_cash_epxense" model="ir.sequence">
        <field name="name">Sequence Petty Cash Expense</field>
        <field name="code">asft.pettycash.expense</field>
        <field name="prefix">PTC/EXP/%(range_year)s/%(month)s/</field>
        <field name="padding">3</field>
        <field name="use_date_range" eval="True"/>
    </record>
</odoo>